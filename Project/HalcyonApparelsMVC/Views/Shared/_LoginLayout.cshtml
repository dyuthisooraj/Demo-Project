<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no"  />
    <title>@ViewData["Title"] - HalcyonApparelsMVC</title>
    
    @*<link href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet">*@
    <link rel="stylesheet" href="~/css/nav.css" />
    <link rel="stylesheet" href="~/css/nav1.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />

    <link rel="stylesheet" href="~/HalcyonApparelsMVC.styles.css" asp-append-version="true" />


    


</head>
<body>
    <header>



        @*<nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom   box-shadow mb-3">*@
        @*<nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light navbar-inverse border-bottom box-shadow mb-3">*@
        <nav class="navbar navbar-expand-lg navbar-inverse ">
            
            <div class="container-fluid">
                <a class="navbar-brand nav-anchor" asp-area="" asp-controller="home" asp-action="index" >Halcyon Apparels</a>
                
            </div>
            
        </nav>

    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

  @*  <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2022 - HalcyonApparelsMVC - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>*@
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>